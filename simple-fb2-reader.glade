                          <object class="GtkMenu" id="menu2">
                            <property name="visible">True</property>
                            <property name="can_focus">False</property>
                            <child>
                              <object class="GtkCheckMenuItem" id="book_color_dark_scheme_checkmenuitem">
                                <property name="use_action_appearance">False</property>
                                <property name="visible">True</property>
                                <property name="can_focus">False</property>
                                <property name="label" translatable="yes">Use dark color scheme</property>
                                <property name="use_underline">True</property>
                                <signal name="toggled" handler="settings_color_dark_scheme_checkmenuitem_toggled_cb" swapped="no"/>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkImageMenuItem" id="book_position_imagemenuitem">
                        <property name="label" translatable="yes">To last read position</property>
                        <property name="use_action_appearance">False</property>
                        <property name="visible">True</property>
                        <property name="can_focus">False</property>
                        <property name="image">book_position_icon_image</property>
                        <property name="use_stock">False</property>
                        <signal name="activate" handler="book_position_imagemenuitem_activate_cb" swapped="no"/>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
