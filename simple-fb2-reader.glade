    <property name="upper">1000</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
  </object>
  <object class="GtkAdjustment" id="book_textview_vertical_adjustment">
    <property name="upper">10000</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
  </object>
  <object class="GtkTreeStore" id="books_sections_treestore">
    <columns>
      <!-- column-name section_name -->
      <column type="gchararray"/>
      <!-- column-name section_string -->
      <column type="gint"/>
    </columns>
  </object>
  <object class="GtkTextBuffer" id="encode_textbuffer"/>
  <object class="GtkTreeStore" id="encode_treestore">
    <columns>
      <!-- column-name is_encode -->
      <column type="gboolean"/>
      <!-- column-name encode_name -->
      <column type="gchararray"/>
    </columns>
  </object>
  <object class="GtkFileFilter" id="fb2_files_filter">
    <patterns>
      <pattern>*.fb2</pattern>
      <pattern>*.fb2.zip</pattern>
      <pattern>*.txt</pattern>
    </patterns>
  </object>
